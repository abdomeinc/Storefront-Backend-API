{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jasmine": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "off",
        "camelcase": "off",
        "@typescript-eslint/no-unused-vars": "error",
        "@typescript-eslint/camelcase": "off"
    }
}
